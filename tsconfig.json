{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "jsxImportSource": "@emotion/react",
    "paths": {
      "@components/*": ["components/*"],
      "@models/*": ["models/*"],
      "@assets/svgs/index": ["public/assets/svgs/index"],
      "@styled": ["styles/styled/global"],
      "@api/index": ["utils/api/index"],
      "@api/aggregation": ["utils/api/aggregationPipelines"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "next.config.js"],
  "exclude": ["node_modules"]
}
